                  assembly="Lynes.ReportsServer.Core"
                  namespace="Lynes.ReportsServer.Core">

  <class name="ReportData" table="ReportData">
    <id name="Id">
      <generator class="guid" />
    </id>

    <property name="Time" />
    <property name="Latitude" />
    <property name="Longitude" />
    
  </class>

</hibernate-mapping>